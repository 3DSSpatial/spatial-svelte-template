{
  "name": "zea-svelte-template",
  "version": "1.0.0",
  "license": "MIT",
  "dependencies": {
    "@auth0/auth0-spa-js": "^1.19.3",
    "@zeainc/gltf-loader": "^4",
    "@zeainc/zea-cad": "^4.0.1",
    "@zeainc/zea-collab": "^6.0.0",
    "@zeainc/zea-engine": "^4.0.1",
    "@zeainc/zea-ux": "^4.0.1",
    "sha256": "^0.2.0",
    "svelte-accessible-dialog": "^2.1.2"
  },
  "devDependencies": {
    "@babel/core": "^7.13.1",
    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.1.3",
    "@rollup/plugin-typescript": "^8.3.0",
    "@roxi/routify": "^2.15.1",
    "@tsconfig/svelte": "^3.0.0",
    "babel-loader": "^8.2.2",
    "cross-env": "^7.0.2",
    "cssnano": "^5.0.15",
    "fs-extra": "^10.0.0",
    "nollup": "^0.18.7",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.2.8",
    "postcss-import": "^14.0.0",
    "prettier": "^2.2.1",
    "prettier-plugin-svelte": "^2.2.0",
    "rollup": "^2.33.1",
    "rollup-plugin-hot": "^0.1.1",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-svelte": "^7.1.0",
    "rollup-plugin-svelte-hot": "^0.14.1",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-workbox": "^6.2.0",
    "sha256": "^0.2.0",
    "spank": "^1.4.0",
    "spassr": "^2.4.0",
    "svelte": "^3.35.0",
    "svelte-accessible-dialog": "^2.1.2",
    "svelte-check": "^2.3.0",
    "svelte-loader": "^3.0.0",
    "svelte-preprocess": "^4.6.9",
    "tailwindcss": "^3.0.13",
    "tslib": "^2.0.0",
    "typescript": "^4.5.4"
  },
  "scripts": {
    "build": "run-s build:*",
    "build:app": "routify -b && rollup -c",
    "build:static": "spank",
    "dev": "run-p routify rollup",
    "dev:nollup": "run-p routify nollup",
    "nollup": "nollup -c",
    "rollup": "rollup -cw",
    "routify": "routify",
    "serve": "spassr --ssr",
    "to-cleanup": "rm -Rf .routify/ dist/ node_modules/",
    "validate": "svelte-check"
  },
  "routify": {
    "extensions": "svelte,html,svx,md"
  },
  "spassr": {
    "assetsDir": [
      "dist",
      "assets"
    ],
    "script": "dist/build/main.js"
  },
  "spank": {
    "blacklist": [
      "/example/modal/basic/4"
    ]
  }
}
